
{
  "apps": [
    {
      "name": "Health & Wellness schedule Work",
      "script": "artisan",
      "args": ["schedule:work"],
      "instances": "1",
      "wait_ready": true,
      "autorestart": true,
      "interpreter": "php",
      "time": true
    },
    {
      "name": "Health & Wellness - Queue pusher",
      "script": "artisan",
      "args": ["queue:work", "--queue=high,default"],
      "exec_mode": "fork",
      "interpreter": "php",
      "instances": 1
    }
  ]
}

